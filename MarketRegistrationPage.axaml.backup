<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Gumaedaehang.MarketRegistrationPage">
  
  <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
    <Grid Background="White" Margin="20">
      <Grid.RowDefinitions>
        <RowDefinition Height="Auto"/>
        <RowDefinition Height="20"/>
        <RowDefinition Height="Auto"/>
        <RowDefinition Height="30"/>
        <RowDefinition Height="Auto"/>
      </Grid.RowDefinitions>
      
      <!-- 검색 영역 -->
      <Grid Grid.Row="0">
        <Grid.ColumnDefinitions>
          <ColumnDefinition Width="*"/>
          <ColumnDefinition Width="10"/>
          <ColumnDefinition Width="100"/>
        </Grid.ColumnDefinitions>
        
        <Border Grid.Column="0" BorderBrush="#E0E0E0" BorderThickness="1" CornerRadius="4" Height="40">
          <TextBox Text="초코 바나나 시럽 아마토 핸드폰" 
                   Background="Transparent" 
                   BorderThickness="0"
                   VerticalAlignment="Center"
                   Padding="12,0"
                   FontSize="14"/>
        </Border>
        
        <Button Grid.Column="2" 
                Content="검색" 
                Background="#FF6B35" 
                Foreground="White" 
                BorderThickness="0" 
                CornerRadius="4" 
                Height="40"
                FontSize="14"
                FontWeight="SemiBold"/>
      </Grid>
      
      <!-- 상품 카드 그리드 영역 -->
      <Grid Grid.Row="2">
        <Grid.RowDefinitions>
          <RowDefinition Height="Auto"/>
          <RowDefinition Height="20"/>
          <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        
        <!-- 첫 번째 행 -->
        <Grid Grid.Row="0">
          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="20"/>
            <ColumnDefinition Width="*"/>
          </Grid.ColumnDefinitions>
          
          <!-- 첫 번째 상품 카드 -->
          <Border Grid.Column="0" BorderBrush="#E0E0E0" BorderThickness="1" CornerRadius="8" Padding="16">
            <Grid>
              <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
              </Grid.RowDefinitions>
              
              <!-- 상품 이미지 -->
              <Border Grid.Row="0" Background="#F8F8F8" Width="320" Height="117" CornerRadius="20" HorizontalAlignment="Left">
                <Image Source="/images/product1.png" Stretch="UniformToFill"/>
              </Border>
              
              <!-- 상품 정보 -->
              <StackPanel Grid.Row="1" Spacing="8" Margin="0,12,0,0">
                <TextBlock Text="상품명: 초코 바나나 시럽 아마토 핸드폰" 
                           FontSize="13" 
                           FontWeight="Medium"
                           TextWrapping="Wrap"
                           LineHeight="18"/>
                
                <TextBlock Text="가격 : 100,000" 
                           FontSize="13" 
                           FontWeight="SemiBold"/>
                
                <!-- 옵션 창 -->
                <Border BorderBrush="#FF6B35" BorderThickness="1" CornerRadius="4" Height="80">
                  <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
                    <TextBox Text="옵션" 
                             Background="Transparent" 
                             BorderThickness="0"
                             FontSize="12"
                             AcceptsReturn="True"
                             TextWrapping="Wrap"
                             Padding="8"/>
                  </ScrollViewer>
                </Border>
              </StackPanel>
              
              <!-- 피드백 및 링크 -->
              <Grid Grid.Row="2" VerticalAlignment="Bottom">
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="*"/>
                  <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                
                <StackPanel Grid.Column="0" Spacing="6" VerticalAlignment="Bottom">
                  <StackPanel Orientation="Horizontal" Spacing="6">
                    <TextBlock Text="피드백:" FontSize="12" Foreground="#666"/>
                    <TextBlock Text="일반업체 중국어가 있습니다." 
                               FontSize="12" 
                               Foreground="#FF6B35"
                               TextWrapping="Wrap"/>
                  </StackPanel>
                </StackPanel>
                
                <Button Grid.Column="1" 
                        Content="네이버쇼핑 바로가기" 
                        FontSize="11" 
                        Foreground="#0066CC" 
                        Background="Transparent"
                        BorderThickness="0"
                        Cursor="Hand"
                        VerticalAlignment="Bottom"
                        HorizontalAlignment="Right"
                        Padding="0"
                        Margin="0,0,0,6"/>
              </Grid>
            </Grid>
          </Border>
          
          <!-- 두 번째 상품 카드 -->
          <Border Grid.Column="2" BorderBrush="#E0E0E0" BorderThickness="1" CornerRadius="8" Padding="16">
            <Grid>
              <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
              </Grid.RowDefinitions>
              
              <!-- 상품 이미지 -->
              <Border Grid.Row="0" Background="#F8F8F8" Width="320" Height="117" CornerRadius="20" HorizontalAlignment="Left">
                <Image Source="/images/product1.png" Stretch="UniformToFill"/>
              </Border>
              
              <!-- 상품 정보 -->
              <StackPanel Grid.Row="1" Spacing="8" Margin="0,12,0,0">
                <TextBlock Text="상품명: 초코 바나나 시럽 아마토 핸드폰" 
                           FontSize="13" 
                           FontWeight="Medium"
                           TextWrapping="Wrap"
                           LineHeight="18"/>
                
                <TextBlock Text="가격 : 100,000" 
                           FontSize="13" 
                           FontWeight="SemiBold"/>
                
                <!-- 옵션 창 -->
                <Border BorderBrush="#FF6B35" BorderThickness="1" CornerRadius="4" Height="80">
                  <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
                    <TextBox Text="옵션" 
                             Background="Transparent" 
                             BorderThickness="0"
                             FontSize="12"
                             AcceptsReturn="True"
                             TextWrapping="Wrap"
                             Padding="8"/>
                  </ScrollViewer>
                </Border>
              </StackPanel>
              
              <!-- 피드백 및 링크 -->
              <Grid Grid.Row="2" VerticalAlignment="Bottom">
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="*"/>
                  <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                
                <StackPanel Grid.Column="0" Spacing="6" VerticalAlignment="Bottom">
                  <StackPanel Orientation="Horizontal" Spacing="6">
                    <TextBlock Text="피드백:" FontSize="12" Foreground="#666"/>
                    <TextBlock Text="브랜드명이 포함되어있습니다." 
                               FontSize="12" 
                               Foreground="#FF6B35"
                               TextWrapping="Wrap"/>
                  </StackPanel>
                </StackPanel>
                
                <Button Grid.Column="1" 
                        Content="네이버쇼핑 바로가기" 
                        FontSize="11" 
                        Foreground="#0066CC" 
                        Background="Transparent"
                        BorderThickness="0"
                        Cursor="Hand"
                        VerticalAlignment="Bottom"
                        HorizontalAlignment="Right"
                        Padding="0"
                        Margin="0,0,0,6"/>
              </Grid>
            </Grid>
          </Border>
        </Grid>
        
        <!-- 두 번째 행 -->
        <Grid Grid.Row="2">
          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="20"/>
            <ColumnDefinition Width="*"/>
          </Grid.ColumnDefinitions>
          
          <!-- 세 번째 상품 카드 -->
          <Border Grid.Column="0" BorderBrush="#E0E0E0" BorderThickness="1" CornerRadius="8" Padding="16">
            <Grid>
              <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
              </Grid.RowDefinitions>
              
              <!-- 상품 이미지 -->
              <Border Grid.Row="0" Background="#F8F8F8" Width="320" Height="117" CornerRadius="20" HorizontalAlignment="Left">
                <Image Source="/images/product1.png" Stretch="UniformToFill"/>
              </Border>
              
              <!-- 상품 정보 -->
              <StackPanel Grid.Row="1" Spacing="8" Margin="0,12,0,0">
                <TextBlock Text="상품명: 초코 바나나 시럽 아마토 핸드폰" 
                           FontSize="13" 
                           FontWeight="Medium"
                           TextWrapping="Wrap"
                           LineHeight="18"/>
                
                <TextBlock Text="가격 : 100,000" 
                           FontSize="13" 
                           FontWeight="SemiBold"/>
                
                <!-- 옵션 창 -->
                <Border BorderBrush="#FF6B35" BorderThickness="1" CornerRadius="4" Height="80">
                  <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
                    <TextBox Text="옵션" 
                             Background="Transparent" 
                             BorderThickness="0"
                             FontSize="12"
                             AcceptsReturn="True"
                             TextWrapping="Wrap"
                             Padding="8"/>
                  </ScrollViewer>
                </Border>
              </StackPanel>
              
              <!-- 피드백 및 링크 -->
              <Grid Grid.Row="2" VerticalAlignment="Bottom">
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="*"/>
                  <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                
                <StackPanel Grid.Column="0" Spacing="6" VerticalAlignment="Bottom">
                  <StackPanel Orientation="Horizontal" Spacing="6">
                    <TextBlock Text="피드백:" FontSize="12" Foreground="#666"/>
                    <TextBlock Text="일반업체 중국어가 있습니다." 
                               FontSize="12" 
                               Foreground="#FF6B35"
                               TextWrapping="Wrap"/>
                  </StackPanel>
                </StackPanel>
                
                <Button Grid.Column="1" 
                        Content="네이버쇼핑 바로가기" 
                        FontSize="11" 
                        Foreground="#0066CC" 
                        Background="Transparent"
                        BorderThickness="0"
                        Cursor="Hand"
                        VerticalAlignment="Bottom"
                        HorizontalAlignment="Right"
                        Padding="0"
                        Margin="0,0,0,6"/>
              </Grid>
            </Grid>
          </Border>
          
          <!-- 네 번째 상품 카드 -->
          <Border Grid.Column="2" BorderBrush="#E0E0E0" BorderThickness="1" CornerRadius="8" Padding="16">
            <Grid>
              <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
              </Grid.RowDefinitions>
              
              <!-- 상품 이미지 -->
              <Border Grid.Row="0" Background="#F8F8F8" Width="320" Height="117" CornerRadius="20" HorizontalAlignment="Left">
                <Image Source="/images/product1.png" Stretch="UniformToFill"/>
              </Border>
              
              <!-- 상품 정보 -->
              <StackPanel Grid.Row="1" Spacing="8" Margin="0,12,0,0">
                <TextBlock Text="상품명: 초코 바나나 시럽 아마토 핸드폰" 
                           FontSize="13" 
                           FontWeight="Medium"
                           TextWrapping="Wrap"
                           LineHeight="18"/>
                
                <TextBlock Text="가격 : 100,000" 
                           FontSize="13" 
                           FontWeight="SemiBold"/>
                
                <!-- 옵션 창 -->
                <Border BorderBrush="#FF6B35" BorderThickness="1" CornerRadius="4" Height="80">
                  <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
                    <TextBox Text="옵션" 
                             Background="Transparent" 
                             BorderThickness="0"
                             FontSize="12"
                             AcceptsReturn="True"
                             TextWrapping="Wrap"
                             Padding="8"/>
                  </ScrollViewer>
                </Border>
              </StackPanel>
              
              <!-- 피드백 및 링크 -->
              <Grid Grid.Row="2" VerticalAlignment="Bottom">
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="*"/>
                  <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                
                <StackPanel Grid.Column="0" Spacing="6" VerticalAlignment="Bottom">
                  <StackPanel Orientation="Horizontal" Spacing="6">
                    <TextBlock Text="피드백:" FontSize="12" Foreground="#666"/>
                    <TextBlock Text="일반업체 중국어가 있습니다." 
                               FontSize="12" 
                               Foreground="#FF6B35"
                               TextWrapping="Wrap"/>
                  </StackPanel>
                </StackPanel>
                
                <Button Grid.Column="1" 
                        Content="네이버쇼핑 바로가기" 
                        FontSize="11" 
                        Foreground="#0066CC" 
                        Background="Transparent"
                        BorderThickness="0"
                        Cursor="Hand"
                        VerticalAlignment="Bottom"
                        HorizontalAlignment="Right"
                        Padding="0"
                        Margin="0,0,0,6"/>
              </Grid>
            </Grid>
          </Border>
        </Grid>
      </Grid>
      
      <!-- 차트 및 범례 영역 -->
      <Grid Grid.Row="4">
        <Grid.ColumnDefinitions>
          <ColumnDefinition Width="2*"/>
          <ColumnDefinition Width="20"/>
          <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        
        <!-- 차트 영역 -->
        <Border Grid.Column="0" Background="#FFF8F0" CornerRadius="8" Padding="20" Height="300">
          <Grid>
            <Grid.RowDefinitions>
              <RowDefinition Height="20"/>
              <RowDefinition Height="*"/>
              <RowDefinition Height="30"/>
            </Grid.RowDefinitions>
            
            <!-- Y축 화살표 -->
            <Canvas Grid.Row="0" Grid.RowSpan="3" HorizontalAlignment="Left" Width="30">
              <Path Data="M 15,15 L 15,250 M 10,20 L 15,15 L 20,20" 
                    Stroke="#FF6B35" StrokeThickness="2" Fill="#FF6B35"/>
            </Canvas>
            
            <!-- 차트 바들 -->
            <Grid Grid.Row="1" Margin="40,10,10,10">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
              </Grid.ColumnDefinitions>
              
              <!-- 1,000 바 -->
              <Grid Grid.Column="0" VerticalAlignment="Bottom">
                <Border Background="#E0E0E0" Height="60" Margin="8,0" CornerRadius="2"/>
                <Border Background="#FF6B35" Height="45" Margin="8,0" VerticalAlignment="Bottom" CornerRadius="2"/>
              </Grid>
              
              <!-- 10,000 바 -->
              <Grid Grid.Column="1" VerticalAlignment="Bottom">
                <Border Background="#E0E0E0" Height="100" Margin="8,0" CornerRadius="2"/>
                <Border Background="#FF6B35" Height="85" Margin="8,0" VerticalAlignment="Bottom" CornerRadius="2"/>
              </Grid>
              
              <!-- 50,000 바 -->
              <Grid Grid.Column="2" VerticalAlignment="Bottom">
                <Border Background="#E0E0E0" Height="180" Margin="8,0" CornerRadius="2"/>
                <Border Background="#FF6B35" Height="70" Margin="8,0" VerticalAlignment="Bottom" CornerRadius="2"/>
              </Grid>
              
              <!-- 100,000 바 -->
              <Grid Grid.Column="3" VerticalAlignment="Bottom">
                <Border Background="#E0E0E0" Height="140" Margin="8,0" CornerRadius="2"/>
                <Border Background="#FF6B35" Height="120" Margin="8,0" VerticalAlignment="Bottom" CornerRadius="2"/>
              </Grid>
            </Grid>
            
            <!-- X축 라벨 -->
            <Grid Grid.Row="2" Margin="40,0,10,0">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
              </Grid.ColumnDefinitions>
              
              <TextBlock Grid.Column="0" Text="1,000" HorizontalAlignment="Center" FontSize="12" FontWeight="Medium"/>
              <TextBlock Grid.Column="1" Text="10,000" HorizontalAlignment="Center" FontSize="12" FontWeight="Medium"/>
              <TextBlock Grid.Column="2" Text="50,000" HorizontalAlignment="Center" FontSize="12" FontWeight="Medium"/>
              <TextBlock Grid.Column="3" Text="100,000" HorizontalAlignment="Center" FontSize="12" FontWeight="Medium"/>
            </Grid>
          </Grid>
        </Border>
        
        <!-- 범례 및 설명 -->
        <StackPanel Grid.Column="2" Spacing="20" VerticalAlignment="Top">
          <!-- 범례 -->
          <StackPanel Spacing="12">
            <StackPanel Orientation="Horizontal" Spacing="10">
              <Ellipse Width="14" Height="14" Fill="#FF6B35"/>
              <TextBlock Text="주문처리형" FontSize="13" FontWeight="Medium"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Spacing="10">
              <Ellipse Width="14" Height="14" Fill="#FFB366"/>
              <TextBlock Text="밸런스형" FontSize="13" FontWeight="Medium"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Spacing="10">
              <Ellipse Width="14" Height="14" Fill="#FFCC99"/>
              <TextBlock Text="크무비형" FontSize="13" FontWeight="Medium"/>
            </StackPanel>
          </StackPanel>
          
          <!-- 설명 텍스트 -->
          <TextBlock Text="피드백 입니다. 주문처리형 밸런스형 크무비형 초코 바나나 시럽 아마토 핸드폰 일반업체 중국어가 있습니다. 브랜드명이 포함되어 있습니다." 
                     FontSize="12" 
                     TextWrapping="Wrap" 
                     LineHeight="18"
                     Foreground="#666"/>
        </StackPanel>
      </Grid>
    </Grid>
  </ScrollViewer>
  
</UserControl>
