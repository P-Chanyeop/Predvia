    // ë°©ë²• 2: ì§ì ‘ ìŠ¤ë§ˆíŠ¸ìŠ¤í† ì–´ ë§í¬ íŒ¨í„´ìœ¼ë¡œ ì°¾ê¸°
    const allLinks = document.querySelectorAll('a[href*="smartstore.naver.com"], a[href*="brand.naver.com"]');
    
    allLinks.forEach((linkElement) => {
      const link = linkElement.href;
      
