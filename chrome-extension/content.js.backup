// â­ ìŠ¤í† ì–´ë³„ ê³ ìœ  runId ìƒì„±
function makeRunId(storeId) {
  return `${storeId}-${Date.now()}-${Math.random().toString(36).slice(2,8)}`;
}

// â­ ì„œë²„ ìƒíƒœ ì„¤ì • í•¨ìˆ˜
async function setStoreState(storeId, runId, state, lock, expected = 0, progress = 0) {
  try {
    console.log(`ğŸ”§ ${storeId}: ìƒíƒœ ì„¤ì • ì‹œë„ - ${state} (lock: ${lock})`);
    
    const response = await fetch('http://localhost:8080/api/smartstore/state', {
      method: 'POST',
      headers: { 'Content-Type': 'application/json' },
      body: JSON.stringify({
        storeId,
        runId,
        state,
        lock,
        expected,
        progress,
        timestamp: new Date().toISOString()
      })
    });
    
    if (response.ok) {
      console.log(`âœ… ${storeId}: ìƒíƒœ ì„¤ì • ì„±ê³µ - ${state} (lock: ${lock})`);
    } else {
      console.log(`âŒ ${storeId}: ìƒíƒœ ì„¤ì • ì‹¤íŒ¨ - ${response.status}`);
    }
  } catch (error) {
    console.log(`âŒ ${storeId}: ìƒíƒœ ì„¤ì • ì˜¤ë¥˜ - ${error.message}`);
  }
}

