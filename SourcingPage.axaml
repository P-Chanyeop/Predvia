<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="1920" d:DesignHeight="930"
             x:Class="Gumaedaehang.SourcingPage">
    
    <UserControl.Styles>
        <!-- 커스텀 스크롤바 스타일 -->
        <Style Selector="ScrollBar">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="BorderThickness" Value="0"/>
        </Style>
        
        <Style Selector="ScrollBar:vertical">
            <Setter Property="Width" Value="12"/>
        </Style>
        
        <Style Selector="ScrollBar /template/ Track">
            <Setter Property="IsHitTestVisible" Value="True"/>
        </Style>
        
        <Style Selector="ScrollBar /template/ Thumb">
            <Setter Property="Background" Value="#F5945C"/>
            <Setter Property="CornerRadius" Value="10"/>
            <Setter Property="MinHeight" Value="30"/>
            <Setter Property="Opacity" Value="1"/>
            <Setter Property="Margin" Value="1"/>
        </Style>
        
        <Style Selector="ScrollBar /template/ Thumb:pointerover">
            <Setter Property="Background" Value="#F5945C"/>
            <Setter Property="Opacity" Value="1"/>
        </Style>
        
        <Style Selector="ScrollBar /template/ Thumb:pressed">
            <Setter Property="Background" Value="#F5945C"/>
            <Setter Property="Opacity" Value="1"/>
        </Style>
        
        <Style Selector="ScrollBar /template/ Button">
            <Setter Property="IsVisible" Value="False"/>
        </Style>
        
        <Style Selector="ScrollBar /template/ RepeatButton">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="BorderThickness" Value="0"/>
        </Style>
    </UserControl.Styles>
    
    <Grid>
        <!-- 데이터가 없을 때 보여줄 화면 (38.png 기반) -->
        <Grid x:Name="NoDataView" IsVisible="True">
            <StackPanel Spacing="60" 
                        VerticalAlignment="Center" 
                        HorizontalAlignment="Center"
                        MaxWidth="1200"
                        Width="1200">
                
                <!-- 필터 섹션 -->
                <StackPanel Spacing="25">
                    <TextBlock Text="필터" 
                               FontSize="28" 
                               FontWeight="Bold" 
                               Foreground="{DynamicResource ForegroundBrush}"
                               HorizontalAlignment="Left"/>
                    
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        
                        <!-- 가격 입력 영역 -->
                        <StackPanel Grid.Column="0" 
                                    Orientation="Horizontal" 
                                    Spacing="20" 
                                    VerticalAlignment="Center">
                            <!-- 최소 가격 입력 -->
                            <TextBox Width="280" 
                                     Height="55" 
                                     Text="1,000"
                                     Background="#FFE5D3"
                                     BorderThickness="0"
                                     CornerRadius="8"
                                     VerticalContentAlignment="Center"
                                     HorizontalContentAlignment="Center"
                                     FontSize="18"
                                     FontWeight="Medium"/>
                            
                            <TextBlock Text="원 ~" 
                                       VerticalAlignment="Center" 
                                       FontSize="20" 
                                       FontWeight="Medium"
                                       Foreground="{DynamicResource ForegroundBrush}"/>
                            
                            <!-- 최대 가격 입력 -->
                            <TextBox Width="280" 
                                     Height="55" 
                                     Text="100,000"
                                     Background="#FFE5D3"
                                     BorderThickness="0"
                                     CornerRadius="8"
                                     VerticalContentAlignment="Center"
                                     HorizontalContentAlignment="Center"
                                     FontSize="18"
                                     FontWeight="Medium"/>
                            
                            <TextBlock Text="원" 
                                       VerticalAlignment="Center" 
                                       FontSize="20" 
                                       FontWeight="Medium"
                                       Foreground="{DynamicResource ForegroundBrush}"/>
                        </StackPanel>
                        
                        <!-- 설정 버튼 -->
                        <Button Grid.Column="1"
                                Content="설정" 
                                Background="#F47B20" 
                                Foreground="White" 
                                Padding="40,15" 
                                CornerRadius="8"
                                FontSize="18"
                                FontWeight="Medium"
                                BorderThickness="0"
                                Height="55"/>
                    </Grid>
                </StackPanel>
                
                <!-- 수동으로 소싱하기 섹션 -->
                <StackPanel Spacing="25">
                    <TextBlock Text="수동으로 소싱하기" 
                               FontSize="28" 
                               FontWeight="Bold" 
                               Foreground="{DynamicResource ForegroundBrush}"
                               HorizontalAlignment="Left"/>
                    
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        
                        <!-- URL 입력 필드 -->
                        <TextBox Grid.Column="0"
                                 Height="55" 
                                 Watermark="URL을 입력해주세요."
                                 Background="#FFE5D3"
                                 BorderThickness="0"
                                 CornerRadius="8"
                                 VerticalContentAlignment="Center"
                                 FontSize="18"
                                 Padding="20,0"
                                 Margin="0,0,20,0"/>
                        
                        <!-- 페어링 버튼 -->
                        <Button Grid.Column="1"
                                Content="페어링" 
                                Background="#F47B20" 
                                Foreground="White" 
                                Padding="40,15" 
                                CornerRadius="8"
                                FontSize="18"
                                FontWeight="Medium"
                                BorderThickness="0"
                                Height="55"/>
                    </Grid>
                    
                    <!-- 추가하기+ 링크 -->
                    <TextBlock Text="추가하기 +" 
                               FontSize="18" 
                               Foreground="#F47B20" 
                               HorizontalAlignment="Center"
                               Cursor="Hand"
                               FontWeight="Medium"
                               x:Name="AddMoreLink"/>
                </StackPanel>
                
                <!-- 소싱재료 자동찾기 섹션 -->
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    
                    <TextBlock Grid.Column="0"
                               Text="소싱재료 자동찾기" 
                               FontSize="28" 
                               FontWeight="Bold" 
                               Foreground="{DynamicResource ForegroundBrush}"
                               VerticalAlignment="Center"/>
                    
                    <!-- 페어링 버튼 -->
                    <Button Grid.Column="1"
                            Content="페어링" 
                            Background="#F47B20" 
                            Foreground="White" 
                            Padding="40,15" 
                            CornerRadius="8"
                            FontSize="18"
                            FontWeight="Medium"
                            BorderThickness="0"
                            Height="55"/>
                </Grid>
                
                <!-- 메인상품 자동찾기 섹션 -->
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    
                    <TextBlock Grid.Column="0"
                               Text="메인상품 자동찾기" 
                               FontSize="28" 
                               FontWeight="Bold" 
                               Foreground="{DynamicResource ForegroundBrush}"
                               VerticalAlignment="Center"/>
                    
                    <!-- 페어링 버튼 -->
                    <Button Grid.Column="1"
                            Content="페어링" 
                            Background="#F47B20" 
                            Foreground="White" 
                            Padding="40,15" 
                            CornerRadius="8"
                            FontSize="18"
                            FontWeight="Medium"
                            BorderThickness="0"
                            Height="55"/>
                </Grid>
                
                <!-- 엑셀로 추출하기 섹션 -->
                <StackPanel Orientation="Horizontal" 
                            Spacing="15" 
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center">
                    <!-- 엑셀 아이콘 -->
                    <Image Source="avares://Gumaedaehang/images/excel_logo.png" 
                           Width="40" 
                           Height="40"/>
                    
                    <TextBlock Text="엑셀로 추출하기" 
                               FontSize="28" 
                               FontWeight="Bold" 
                               Foreground="{DynamicResource ForegroundBrush}"
                               VerticalAlignment="Center"/>
                </StackPanel>
                
                <!-- 테스트 버튼 (임시) -->
                <Button Content="데이터 있는 화면 보기 (테스트)" 
                        Background="#F47B20" 
                        Foreground="White" 
                        Padding="30,15" 
                        CornerRadius="8"
                        FontSize="16"
                        FontWeight="Medium"
                        HorizontalAlignment="Center"
                        Margin="0,30,0,0"
                        x:Name="TestDataButton"/>
                
            </StackPanel>
        </Grid>
        
        <!-- 데이터가 있을 때 보여줄 화면 (39.png 기반) -->
        <Grid x:Name="DataAvailableView" IsVisible="False">
            <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
                <StackPanel Spacing="20" Margin="30,20,30,30">
                    
                    <!-- 전체선택 및 삭제 영역 -->
                    <StackPanel Orientation="Horizontal" Spacing="15" VerticalAlignment="Center">
                        <CheckBox x:Name="SelectAllCheckBox"
                                  Content="전체선택" 
                                  FontSize="14" 
                                  Foreground="{DynamicResource ForegroundBrush}"/>
                        <TextBlock Text="삭제" 
                                   FontSize="14" 
                                   Foreground="{DynamicResource ForegroundBrush}"
                                   VerticalAlignment="Center"/>
                    </StackPanel>
                    
                    <!-- 카테고리 경로 -->
                    <StackPanel Orientation="Horizontal" Spacing="8" VerticalAlignment="Center" Margin="10,0,0,0">
                        <CheckBox x:Name="Product1CheckBox" 
                                  VerticalAlignment="Center" 
                                  Margin="0,0,8,0"/>
                        <Ellipse Width="6" Height="6" Fill="#F47B20" VerticalAlignment="Center"/>
                        <TextBlock Text="카테고리 : 홈 > 가구 > 침대" 
                                   FontSize="13" 
                                   Foreground="{DynamicResource ForegroundBrush}"
                                   VerticalAlignment="Center"/>
                    </StackPanel>
                    
                    <!-- 상품 1 -->
                    <Border Background="Transparent" 
                            CornerRadius="0" 
                            Padding="15"
                            BorderThickness="0"
                            Margin="10,0,0,0">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="290"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="150"/>
                            </Grid.ColumnDefinitions>
                            
                            <!-- 상품 이미지 -->
                            <Border Grid.Column="0" 
                                    Width="260" 
                                    Height="260" 
                                    Background="#F0F0F0" 
                                    CornerRadius="20">
                                <Image Source="avares://Gumaedaehang/images/predvia_logo.png"
                                       Stretch="Uniform"
                                       Margin="20"/>
                            </Border>
                            
                            <!-- 상품 정보 -->
                            <StackPanel Grid.Column="1" Margin="30,0,10,0" VerticalAlignment="Top">
                                <!-- 상품명 -->
                                <StackPanel Orientation="Horizontal" Spacing="8" Margin="0,0,0,15">
                                    <!-- 바이트 수 표시등 -->
                                    <Ellipse x:Name="ProductNameStatusIndicator"
                                             Width="12" 
                                             Height="12" 
                                             Fill="#53DA4C"
                                             VerticalAlignment="Center"/>
                                    
                                    <TextBlock Text="상품명 :" 
                                               FontSize="15" 
                                               FontWeight="Medium"
                                               Foreground="{DynamicResource ForegroundBrush}"
                                               VerticalAlignment="Center"/>
                                    
                                    <!-- 키워드 태그들 (박스 없이 직접 표시) -->
                                    <WrapPanel x:Name="ProductNameKeywordPanel" 
                                               Orientation="Horizontal"
                                               MaxWidth="800"
                                               VerticalAlignment="Center"
                                               HorizontalAlignment="Left"/>
                                    
                                    <TextBlock x:Name="ByteCountTextBlock"
                                               Text="0/50 byte" 
                                               FontSize="12" 
                                               Foreground="Gray"
                                               VerticalAlignment="Center"
                                               HorizontalAlignment="Right"/>
                                </StackPanel>
                                
                                <!-- 원상품명 -->
                                <StackPanel Orientation="Horizontal" Spacing="8" Margin="0,0,0,18">
                                    <TextBlock Text="원상품명:" 
                                               FontSize="15" 
                                               FontWeight="Medium"
                                               Foreground="{DynamicResource ForegroundBrush}"/>
                                    <TextBlock Text="초코 바나나 시럽 사탕 아이스크림" 
                                               FontSize="15" 
                                               Foreground="{DynamicResource ForegroundBrush}"/>
                                </StackPanel>
                                
                                <!-- 키워드 태그들 -->
                                <WrapPanel x:Name="KeywordPanel1" Margin="0,0,0,18">
                                    <Border x:Name="Keyword1_1" Background="#F47B20" CornerRadius="12" Padding="8,4" Margin="0,0,6,4" Cursor="Hand">
                                        <TextBlock Text="카페드345" FontSize="12" Foreground="White"/>
                                    </Border>
                                    <Border x:Name="Keyword1_2" Background="#D0D0D0" CornerRadius="12" Padding="8,4" Margin="0,0,6,4" Cursor="Hand">
                                        <TextBlock Text="카페드553422" FontSize="12" Foreground="Gray"/>
                                    </Border>
                                    <Border x:Name="Keyword1_3" Background="#F47B20" CornerRadius="12" Padding="8,4" Margin="0,0,6,4" Cursor="Hand">
                                        <TextBlock Text="카페드1" FontSize="12" Foreground="White"/>
                                    </Border>
                                    <Border x:Name="Keyword1_4" Background="#F47B20" CornerRadius="12" Padding="8,4" Margin="0,0,6,4" Cursor="Hand">
                                        <TextBlock Text="바나나" FontSize="12" Foreground="White"/>
                                    </Border>
                                    <Border x:Name="Keyword1_5" Background="#F47B20" CornerRadius="12" Padding="8,4" Margin="0,0,6,4" Cursor="Hand">
                                        <TextBlock Text="시럽" FontSize="12" Foreground="White"/>
                                    </Border>
                                    <Border x:Name="Keyword1_6" Background="#F47B20" CornerRadius="12" Padding="8,4" Margin="0,0,6,4" Cursor="Hand">
                                        <TextBlock Text="사탕" FontSize="12" Foreground="White"/>
                                    </Border>
                                    <Border x:Name="Keyword1_7" Background="#F47B20" CornerRadius="12" Padding="8,4" Margin="0,0,6,4" Cursor="Hand">
                                        <TextBlock Text="아이스크림" FontSize="12" Foreground="White"/>
                                    </Border>
                                </WrapPanel>
                                
                                <!-- 키워드 추가 입력 섹션 -->
                                <StackPanel Orientation="Horizontal" Spacing="10" Margin="0,0,0,15">
                                    <TextBox x:Name="KeywordInputBox"
                                             FontSize="14"
                                             Width="200"
                                             Height="32"
                                             Padding="12,6"
                                             Background="{DynamicResource BackgroundBrush}"
                                             Foreground="{DynamicResource ForegroundBrush}"
                                             BorderBrush="{DynamicResource BorderBrush}"
                                             BorderThickness="1"
                                             CornerRadius="6"
                                             Watermark="키워드 입력"
                                             AcceptsReturn="False"
                                             AcceptsTab="False"
                                             TextWrapping="NoWrap"/>
                                    
                                    <Button x:Name="AddKeywordButton"
                                            Content="추가"
                                            FontSize="14"
                                            Width="60"
                                            Height="32"
                                            Background="#F47B20"
                                            Foreground="White"
                                            BorderThickness="0"
                                            CornerRadius="6"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            Cursor="Hand"/>
                                </StackPanel>
                            </StackPanel>
                            
                            <!-- 우측 버튼들 -->
                            <StackPanel Grid.Column="2" VerticalAlignment="Top" Spacing="12" Margin="0,0,0,0">
                                <Button Content="상세" 
                                        Background="#F47B20" 
                                        Foreground="White" 
                                        Padding="30,8" 
                                        CornerRadius="6"
                                        FontSize="14"
                                        HorizontalAlignment="Stretch"
                                        BorderThickness="0"/>
                                <Button Content="상품분류" 
                                        Background="#E0E0E0" 
                                        Foreground="Black" 
                                        Padding="30,8" 
                                        CornerRadius="6"
                                        FontSize="14"
                                        HorizontalAlignment="Stretch"
                                        BorderThickness="0"/>
                            </StackPanel>
                        </Grid>
                    </Border>
                    
                    <!-- 리뷰 정보 섹션 -->
                    <Border Background="Transparent" 
                            BorderBrush="#FFDAC4"
                            BorderThickness="2" 
                            CornerRadius="20" 
                            Padding="15" 
                            Margin="25,0,0,0"
                            Width="1415"
                            Height="158"
                            HorizontalAlignment="Left">
                        <ScrollViewer Height="128" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
                                <StackPanel Spacing="5">
                                    <StackPanel Orientation="Horizontal" Spacing="5">
                                        <TextBlock Text="리뷰 : 프레디비아3213 : 제품이 너무 좋아요" 
                                                   FontSize="12" 
                                                   Foreground="{DynamicResource ForegroundBrush}"/>
                                        <TextBlock Text="★★★★★" FontSize="12" Foreground="#F47B20"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Spacing="5">
                                        <TextBlock Text="리뷰 : 사용자1234 : 배송이 빠르고 품질이 우수합니다" 
                                                   FontSize="12" 
                                                   Foreground="{DynamicResource ForegroundBrush}"/>
                                        <TextBlock Text="★★★★☆" FontSize="12" Foreground="#F47B20"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Spacing="5">
                                        <TextBlock Text="리뷰 : 구매자567 : 가격 대비 만족스러운 상품" 
                                                   FontSize="12" 
                                                   Foreground="{DynamicResource ForegroundBrush}"/>
                                        <TextBlock Text="★★★★★" FontSize="12" Foreground="#F47B20"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Spacing="5">
                                        <TextBlock Text="리뷰 : 고객890 : 디자인이 예쁘고 실용적이에요" 
                                                   FontSize="12" 
                                                   Foreground="{DynamicResource ForegroundBrush}"/>
                                        <TextBlock Text="★★★★☆" FontSize="12" Foreground="#F47B20"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Spacing="5">
                                        <TextBlock Text="리뷰 : 리뷰어111 : 재구매 의사 있습니다" 
                                                   FontSize="12" 
                                                   Foreground="{DynamicResource ForegroundBrush}"/>
                                        <TextBlock Text="★★★★★" FontSize="12" Foreground="#F47B20"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Spacing="5">
                                        <TextBlock Text="리뷰 : 만족고객 : 추천하고 싶은 상품입니다" 
                                                   FontSize="12" 
                                                   Foreground="{DynamicResource ForegroundBrush}"/>
                                        <TextBlock Text="★★★★☆" FontSize="12" Foreground="#F47B20"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Spacing="5">
                                        <TextBlock Text="리뷰 : 만족고객 : 추천하고 싶은 상품입니다" 
                                                   FontSize="12" 
                                                   Foreground="{DynamicResource ForegroundBrush}"/>
                                        <TextBlock Text="★★★★☆" FontSize="12" Foreground="#F47B20"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Spacing="5">
                                        <TextBlock Text="리뷰 : 만족고객 : 추천하고 싶은 상품입니다" 
                                                   FontSize="12" 
                                                   Foreground="{DynamicResource ForegroundBrush}"/>
                                        <TextBlock Text="★★★★☆" FontSize="12" Foreground="#F47B20"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Spacing="5">
                                        <TextBlock Text="리뷰 : 만족고객 : 추천하고 싶은 상품입니다" 
                                                   FontSize="12" 
                                                   Foreground="{DynamicResource ForegroundBrush}"/>
                                        <TextBlock Text="★★★★☆" FontSize="12" Foreground="#F47B20"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Spacing="5">
                                        <TextBlock Text="리뷰 : 만족고객 : 추천하고 싶은 상품입니다" 
                                                   FontSize="12" 
                                                   Foreground="{DynamicResource ForegroundBrush}"/>
                                        <TextBlock Text="★★★★☆" FontSize="12" Foreground="#F47B20"/>
                                    </StackPanel>
                            </StackPanel>
                        </ScrollViewer>
                    </Border>
                    
                    <!-- 타오바오와 페이링 섹션 -->
                    <StackPanel Spacing="12" Margin="25,10,0,0">
                        <StackPanel Orientation="Horizontal" Spacing="12" VerticalAlignment="Center">
                            <!-- 타오바오 페어링 상태 표시등 -->
                            <Ellipse x:Name="TaobaoPairingStatusIndicator"
                                     Width="12" 
                                     Height="12" 
                                     Fill="#FF7272"
                                     VerticalAlignment="Center"/>
                            
                            <TextBlock Text="타오바오와 페어링" 
                                       FontSize="16" 
                                       FontWeight="Medium" 
                                       Foreground="{DynamicResource ForegroundBrush}"/>
                            <Button x:Name="TaobaoPairingButton"
                                    Content="페어링" 
                                    Background="#F47B20" 
                                    Foreground="White" 
                                    Padding="20,5" 
                                    CornerRadius="4"
                                    FontSize="13"
                                    BorderThickness="0"/>
                        </StackPanel>
                        
                        <!-- 하단 상품 이미지들 -->
                        <StackPanel Orientation="Horizontal" Spacing="15" HorizontalAlignment="Left">
                            <Border Width="250" Height="120" Background="#F0F0F0" CornerRadius="20">
                                <Image Source="avares://Gumaedaehang/images/predvia_logo.png" 
                                       Stretch="Uniform" 
                                       Margin="8"/>
                            </Border>
                            <Border Width="250" Height="120" Background="#F0F0F0" CornerRadius="20">
                                <Image Source="avares://Gumaedaehang/images/predvia_logo.png" 
                                       Stretch="Uniform" 
                                       Margin="8"/>
                            </Border>
                            <Border Width="250" Height="120" Background="#F0F0F0" CornerRadius="20">
                                <Image Source="avares://Gumaedaehang/images/predvia_logo.png" 
                                       Stretch="Uniform" 
                                       Margin="8"/>
                            </Border>
                            <Border Width="250" Height="120" Background="#F0F0F0" CornerRadius="20">
                                <Image Source="avares://Gumaedaehang/images/predvia_logo.png" 
                                       Stretch="Uniform" 
                                       Margin="8"/>
                            </Border>
                            <Border Width="250" Height="120" Background="#F0F0F0" CornerRadius="20">
                                <Image Source="avares://Gumaedaehang/images/predvia_logo.png" 
                                       Stretch="Uniform" 
                                       Margin="8"/>
                            </Border>
                        </StackPanel>
                    </StackPanel>
                    
                    <!-- 테스트 버튼 (데이터 있는 화면에서도) -->
                    <Button Content="데이터 없는 화면 보기 (테스트)" 
                            Background="#F47B20" 
                            Foreground="White" 
                            Padding="30,15" 
                            CornerRadius="8"
                            FontSize="16"
                            FontWeight="Medium"
                            HorizontalAlignment="Center"
                            Margin="0,30,0,0"
                            x:Name="TestDataButton2"/>
                    
                </StackPanel>
            </ScrollViewer>
        </Grid>
    </Grid>
</UserControl>
